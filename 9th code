public class ContinueExample {
    public static void main(String[] args) {
        System.out.println("Numbers from 1 to 10, skipping 5:");

        for (int i = 1; i <= 10; i++) {
            if (i == 5) {
                continue;  // Skip the number 5
            }
            System.out.println(i);
        }
    }
}
